{
  "name": "react-router-tailwind-starter",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "check:unused": "knip",
    "clean": "rm -rf node_modules",
    "dev": "react-router dev",
    "format": "pnpm exec biome format --write",
    "lint": "pnpm exec biome lint --write",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc"
  },
  "sideEffects": [
    "*.css"
  ],
  "dependencies": {
    "@react-router/node": "^7.1.3",
    "@react-router/serve": "^7.1.3",
    "@vercel/react-router": "^1.0.2",
    "clsx": "^2.1.1",
    "isbot": "^5.1.17",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.1.3",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.5",
    "@react-router/dev": "^7.1.3",
    "@tailwindcss/vite": "^4.1.5",
    "@types/node": "^20",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "autoprefixer": "^10.4.20",
    "dotenv": "16.5.0",
    "globals": "^16.0.0",
    "knip": "^5.69.1",
    "postcss": "^8.4.49",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.5",
    "tsx": "4.19.3",
    "typescript": "^5.7.2",
    "vite": "^5.4.19",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "engines": {
    "node": ">=20.10.0"
  }
}
