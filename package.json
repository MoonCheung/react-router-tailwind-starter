{
  "name": "react-router-tailwind-starter",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "check:unused": "knip",
    "clean": "rm -rf node_modules",
    "dev": "react-router dev",
    "format": "pnpm exec biome format --write",
    "lint": "pnpm exec biome lint --write",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "db:migrate:dev": "npx env-cmd -f .env.local npx prisma migrate dev",
    "db:push:dev": "npx env-cmd -f .env.local npx prisma db push",
    "db:generate": "npx env-cmd -f .env.local npx prisma generate",
    "db:studio": "npx env-cmd -f .env.local npx prisma studio",
    "db:seed": "npx tsx prisma/seed.ts"
  },
  "sideEffects": [
    "*.css"
  ],
  "dependencies": {
    "@conform-to/react": "^1.13.3",
    "@conform-to/zod": "^1.13.3",
    "@pothos/core": "^4.10.0",
    "@pothos/plugin-prisma": "^4.13.0",
    "@pothos/plugin-relay": "^4.6.2",
    "@pothos/plugin-scope-auth": "^4.1.6",
    "@pothos/plugin-simple-objects": "^4.1.3",
    "@prisma/client": "^6.19.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-form": "^0.1.8",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-router/fs-routes": "^7.9.6",
    "@react-router/node": "^7.1.3",
    "@react-router/serve": "^7.1.3",
    "@vercel/react-router": "^1.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "graphql": "^16.12.0",
    "graphql-request": "^7.3.5",
    "graphql-scalars": "^1.25.0",
    "graphql-yoga": "^5.17.1",
    "isbot": "^5.1.17",
    "lucide-react": "^0.554.0",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-call": "^1.8.1",
    "react-dom": "^19.0.0",
    "react-router": "^7.1.3",
    "remix-auth": "^4.2.0",
    "remix-auth-form": "^3.0.0",
    "remix-auth-oauth2": "^3.4.1",
    "sonner": "^2.0.7",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.5",
    "@faker-js/faker": "^10.1.0",
    "@react-router/dev": "^7.1.3",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.5",
    "@types/node": "^20",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "autoprefixer": "^10.4.20",
    "dotenv": "16.5.0",
    "globals": "^16.0.0",
    "knip": "^5.69.1",
    "postcss": "^8.4.49",
    "prisma": "^6.19.1",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.5",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "4.19.3",
    "typescript": "^5.7.2",
    "vite": "^5.4.19",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "engines": {
    "node": ">=20.10.0"
  }
}
